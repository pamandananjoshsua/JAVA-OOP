public class Encapsulation1 {
    String title;
    String description;
    String author;
    public Encapsulation1(String title,  String description, String author){
        this.title = title;
        this.description = description;
        this.author = author;
    }
    String getTitle(){
        return title;
    }
    String getDescription(){
        return description;
    }
    String getAuthor(){
        return author;
    }
}
